{
  "manifest_version": 3,
  "name": "Whoogle Search",
  "version": "1.2",
  "author": "PrestonHunter",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "whoogleicon.png"
  },
  "browser_action": {
    "default_title": "Whoogle"
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "64.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Whoogle",
      "keyword": "@whoogle, @wg, @who, @wgl",
      "search_url": "http://whoogle.phx.hive.io:5000/search?q={searchTerms}",
      "favicon_url": "whoogleicon.png",
      "suggest_url": "http://whoogle.phx.hive.io:5000/search?q={searchTerms}",
      "is_default": true
    }
  }
}
